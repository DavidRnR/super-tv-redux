{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "comma-dangle":"off",
        "indent": ["warn", 4],
        "no-unused-vars":"warn",
        "react/prop-types": "off",
        "semi": [2, "always"],
        "no-debugger": "warn"
    }
}
